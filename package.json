{
  "name": "pull-pixi-sprite-event",
  "version": "0.1.2",
  "description": "A pull-stream through that will throttle the source until a Pixi.js sprite event happens",
  "main": "index.js",
  "repository": "git@github.com:20kbudget/pull-pixi-sprite-event.git",
  "author": "Fabricio C Zuardi <fabricio@fabricio.org>",
  "license": "MIT",
  "keywords": [
    "pull-stream",
    "pixi",
    "pixi.js",
    "events"
  ],
  "devDependencies": {
    "browserify": "^14.1.0",
    "es2020": "^1.1.9",
    "prettier": "^0.20.0"
  },
  "scripts": {
    "format": "prettier --write --single-quote --tab-width 4 --",
    "build" : "browserify index.js -t es2020 -o dist/index.js",
    "postbuild": "cp README.md LICENSE dist/."
  }
}
